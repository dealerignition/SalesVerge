%div.page-header
  %h1 Account Settings

%div.row
  = render 'navigation'
    
  %div.span9
    %h3 Users
    %table.table.table-striped
      - @dealer.users.each do |user|
        %tr
          %td
            = user.first_name
            = user.last_name
          %td= user.email
          %td= link_to "Deactivate", "#", { :class => "btn btn-danger", :confirm => "Are you sure you want to deactivate #{user.first_name}?" }
    %h3 Create new user
    = render 'users/admin_form'